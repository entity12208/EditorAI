{
  "geode": "4.10.0",
  "gd": {
    "win": "2.2074",
    "android": "2.2074",
    "mac": "2.2074",
    "ios": "2.2074"
  },
  "id": "entity12208.edit-ai",
  "name": "Editor AI",
  "version": "v2.1.1",
  "developer": "entity12208",
  "description": "Generate Geometry Dash levels using AI! Supports Gemini, Claude, and ChatGPT with full trigger support (Alpha, Move, Toggle) and group IDs. Now available on all platforms!",
  "settings": {
    "ai-provider": {
      "type": "string",
      "name": "AI Provider",
      "description": "Which AI service to use",
      "default": "gemini",
      "one-of": ["gemini", "claude", "openai"]
    },
    "model": {
      "type": "string",
      "name": "Model",
      "description": "AI model to use",
      "default": "gemini-2.5-flash",
      "one-of": [
        "gemini-2.5-flash",
        "gemini-2.5-pro",
        "gemini-2.5-flash-lite",
        "claude-sonnet-4-5-20250929",
        "claude-haiku-4-5-20251001",
        "claude-opus-4-5-20251101",
        "gpt-5.2-2025-12-11",
        "gpt-5-mini-2025-08-07",
        "gpt-5-nano-2025-08-07"
      ]
    },
    "difficulty": {
      "type": "string",
      "name": "Difficulty",
      "description": "Default difficulty level",
      "default": "medium",
      "one-of": ["easy", "medium", "hard", "extreme"]
    },
    "style": {
      "type": "string",
      "name": "Style",
      "description": "Visual style preference",
      "default": "modern",
      "one-of": ["modern", "retro", "minimalist", "decorated"]
    },
    "length": {
      "type": "string",
      "name": "Length",
      "description": "Approximate level length",
      "default": "medium",
      "one-of": ["short", "medium", "long", "xl", "xxl"]
    },
    "max-objects": {
      "type": "int",
      "name": "Max Objects",
      "description": "Maximum objects per generation",
      "default": 500,
      "min": 10,
      "max": 1000000
    },
    "rate-limiting-section": {
      "type": "title",
      "name": "Rate Limiting"
    },
    "enable-rate-limiting": {
      "type": "bool",
      "name": "Enable Rate Limiting",
      "description": "Prevents excessive API calls to save tokens and avoid rate limits",
      "default": true
    },
    "rate-limit-seconds": {
      "type": "int",
      "name": "Minimum Seconds Between Requests",
      "description": "Minimum time between API requests (prevents spam)",
      "default": 3,
      "min": 1,
      "max": 60,
      "enableIf": "enable-rate-limiting"
    },
    "features-section": {
      "type": "title",
      "name": "Features"
    },
    "enable-colors": {
      "type": "bool",
      "name": "AI Color Control",
      "description": "Allow AI to set object colors",
      "default": false
    },
    "enable-group-ids": {
      "type": "bool",
      "name": "AI Group IDs",
      "description": "Allow AI to assign Group IDs to objects",
      "default": false
    },
    "enable-triggers": {
      "type": "bool",
      "name": "Trigger Support âš¡",
      "description": "AI can create and configure Alpha, Move, and Toggle triggers with touch activation",
      "default": false
    },
    "api-key-info": {
      "type": "title",
      "name": "API Key Management"
    },
    "api-key-status": {
      "type": "string",
      "name": "Status",
      "description": "Current API key status",
      "default": "No API key saved",
      "enableIf": "false"
    }
  },
  "platforms": [
    "win",
    "android32",
    "android64",
    "mac",
    "ios"
  ],
  "tags": [
    "editor",
    "utility",
    "content"
  ],
  "links": {
    "source": "https://github.com/Entity12208/EditorAI",
    "community": "https://discord.gg/geometrydash"
  }
}
